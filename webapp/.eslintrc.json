{
  "extends": "airbnb",
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "object-curly-newline": ["error", { "multiline": true }],
    "arrow-parens": ["error", "as-needed"],
    "jsx-quotes": ["error", "prefer-single"]
  },
  "globals": {
    "document": true,
    "fetch": true
  }
}
